<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.ecmdeveloper.plugin.scripting.handlers.ExecuteMethodHandler"
            id="com.ecmdeveloper.plugin.scripting.executeClass"
            name="Execute Script">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:com.ecmdeveloper.plugin.views.ObjectStoresView?after=additions">
         <command
               commandId="com.ecmdeveloper.plugin.scripting.executeClass"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
