            commandId="org.eclipse.ui.edit.delete">
         <enabledWhen>
            <with
                  variable="selection">
               <iterate
                     ifEmpty="false">
                  <or>
                     <instanceof
                           value="com.ecmdeveloper.plugin.core.model.IFolder">
                     </instanceof>
                     <instanceof
                           value="com.ecmdeveloper.plugin.core.model.IDocument">
                     </instanceof>
                     <instanceof
                           value="com.ecmdeveloper.plugin.core.model.ICustomObject">
                     </instanceof>
                  </or>
