            </with>
         </enabledWhen>
      </handler>
      <handler
            class="com.ecmdeveloper.plugin.ui.handlers.ConnectObjectStoreHandler"
            commandId="com.ecmdeveloper.plugin.connectObjectStore">
         <enabledWhen>
            <with
                  variable="selection">
