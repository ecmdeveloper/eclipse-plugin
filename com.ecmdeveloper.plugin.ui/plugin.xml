<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.ecmdeveloper.plugin.ui.perspectives.ContentEnginePerspectiveFactory"
            icon="icons/database_table.png"
            id="com.ecmdeveloper.plugin.ui.contentEnginePerspective"
            name="Content Engine">
         <description>
            This perspective is designed to support working with Content Engines. It offers an Object Store explorer, a Favorities view, a Code Modules view and a Tracker view.
         </description>
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="Commands related to the Content Engine"
            id="com.ecmdeveloper.plugin.category"
            name="Content Engine Commands">
      </category>
      <command
            categoryId="com.ecmdeveloper.plugin.category"
            description="Deletes the selected item from the Object Store"
            id="com.ecmdeveloper.plugin.delete"
            name="Delete">
      </command>
      <command
            categoryId="com.ecmdeveloper.plugin.category"
            description="Connect to the Object Store"
            id="com.ecmdeveloper.plugin.connectObjectStore"
            name="Connect">
      </command>
      <command
            categoryId="com.ecmdeveloper.plugin.category"
            description="Removes this Object Store from the view"
            id="com.ecmdeveloper.plugin.removeObjectStore"
            name="Remove from view">
      </command>
      <command
            categoryId="com.ecmdeveloper.plugin.category"
            description="Moves the selected items to another location in the Object Store"
            id="com.ecmdeveloper.plugin.move"
            name="Move...">
      </command>
      <command
            description="Paste the clipboard contents into the Object Stores view"
            id="com.ecmdeveloper.plugin.paste"
            name="Past">
      </command>
      <command
            categoryId="com.ecmdeveloper.plugin.category"
            description="Renames the selected items"
            id="com.ecmdeveloper.plugin.rename"
            name="Rename...">
      </command>
      <command
            categoryId="com.ecmdeveloper.plugin.category"
            description="Refreshes the selected items"
            id="com.ecmdeveloper.plugin.refresh"
            name="Refresh">
      </command>
      <command
            description="Import Object Store"
            id="com.ecmdeveloper.plugin.importObjectStore"
            name="Import`">
      </command>
      <command
            description="Delete Versions of the Document"
            id="com.ecmdeveloper.plugin.deleteDocumentVersion"
            name="Delete Versions...">
      </command>
      <command
            defaultHandler="com.ecmdeveloper.plugin.ui.handlers.ObjectStoresViewDoubleClickHandler"
            id="com.ecmdeveloper.plugin.objectStoresViewDoubleClick"
            name="DoubleClick">
      </command>
      <command
            id="com.ecmdeveloper.plugin.ui.importObjectStore"
            name="Import Object Store">
      </command>
   </extension>
